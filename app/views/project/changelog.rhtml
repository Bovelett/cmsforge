<div class="content">
<% @curloc = 'changelog' -%>

<h4>Changelog</h4>

<p><pre><%= h @project.changelog -%></pre></p>

<h3>Previous Releases</h3>
<% for release in @project.releases %>
  <dt><%= h release.name -%></dt>
  <dd><pre><%= h release.changelog -%></pre></dd>
<% end %>
</div>
