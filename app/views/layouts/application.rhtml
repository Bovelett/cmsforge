    /* Main navigation bar */
    #content #mainnav {
      position: relative;
      width: 100%;
      vertical-align: top;
      text-align: center;
      margin-top: 1px;
      border-top: 1px solid #ccc;
    }

    #content #mainnav ul, #content #mainnav li {
      margin: 0;
      padding: 0;
      display: inline;
      list-style: none;
    }

    #content #mainnav :link, #content #mainnav :visited {
      padding: 5px 6px;
      line-height: 25px;
      font-size: 1em;
      font-weight: normal;
      text-decoration: none;
      border-right: 1px solid #C1C1C1;
      border-left: 1px solid #C1C1C1;
      border-bottom: 1px solid #C1C1C1;
      color: #0F337F;
      background-color: #fff;
    }

    * html #content #mainnav :link, * html #content #mainnav :visited { background-position: 1px 0 }

    #content #mainnav :link:hover, #content #mainnav :visited:hover {
      background-color: #E7AB0B;
      border-right: 1px solid #ddd;
      color: #fff;
    }

    #content #mainnav .active :link, #content #mainnav .active :visited {
      background: #30529C;
      border-top: none;
      border-right: 1px solid #000;
      color: #eee;
      font-weight: normal;
    }

    #content #mainnav .active :link:hover, #content #mainnav .active :visited:hover {
      border-right: 1px solid #000;
    }


