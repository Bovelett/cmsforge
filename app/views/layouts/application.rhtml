<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <title>CMSMS Forge: Welcome</title>
  <link rel="alternate" title="CMSMS Forge - Project News Highlights RSS" href="/export/rss_sfnews.php" type="application/rss+xml"/>
  <link rel="alternate" title="CMSMS Forge - New Projects RSS" href="/export/rss_sfprojects.php" type="application/rss+xml"/>

  <script type="text/javascript">
  <!--
  //pass min and max -measured against window width
  function P7_MinMaxW(a,b){
    var nw="auto",w=document.documentElement.clientWidth;
    if(w>=b){nw=b+"px";}if(w<=a){nw=a+"px";}return nw;
  }
  //-->
  </script>

  <!--[if lte IE 6]>
    <style type="text/css">
      #pagewrapper {width:expression(P7_MinMaxW(775,950));}
      #container {height: 1%;}
    </style>
    <![endif]-->

    <!-- <link rel="alternate" type="application/rss+xml" title="RSS 2.0" href="http://cmsmadesimple.org/News/rss/" /> -->

    <script type="text/javascript">
    <!--

    function admin_window(adminurl) {
      AdminWin = window.open( adminurl, 'AdminWindow','scrollbars=yes,resizable=yes, toolbar=yes, height=400, width=400, top=2, left=2');
      AdminWin.focus();
    }
    function help_window(helpurl) {
      HelpWin = window.open( helpurl,'HelpWindow','scrollbars=yes,resizable=yes,toolbar=no,height=400,width=400');
    }

    // -->
    </script>

    <!--link rel="stylesheet" type="text/css" href="/stylesheets/cmsforge.css" /-->

    <%= javascript_include_tag :defaults %>
    <%= stylesheet_link_tag 'cmsforge.css' %>

  </head>
  <body>
    <div id="pagewrapper">
      <div id="container">

        <!-- start accessibility skip links -->
        <ul class="accessibility">

          <li><a href="#menu_vert" accesskey="s" title="Skip to navigation">Skip to navigation</a></li>
          <li><a href="/News/57/8#main">Skip to content</a></li>
        </ul>

        <!-- end accessibility skip links -->

        <hr />


        <!-- Start Header, with logo image that links to the default start page -->

        <div id="page_top" class="clearfix">
          <div id="header">
            <div id="header_left">
              <h1><a href="http://cmsmadesimple.org/main/home" title="Welcome to CMS Made Simple">Welcome to CMS Made Simple</a></h1>

            </div>


            <!-- Start Horizontal Menu -->

            <div id="header-right">
              <div id="menu_horiz">
                <h2 class="accessibility">Main Navigation</h2>
                <div id="top-nav">


                  <ul>
                    <li><a href="http://cmsmadesimple.org" ><dfn>1: </dfn>Main</a></li>
                    <li><a href="http://blog.cmsmadesimple.org/" ><dfn>2: </dfn>Blog</a></li>
                    <li><a href="http://cmsmadesimple.org/downloads" ><dfn>3: </dfn>Downloads</a></li>
                    <li><a href="http://wiki.cmsmadesimple.org/" ><dfn>4: </dfn>Documentation</a></li>

                    <li><a href="http://forum.cmsmadesimple.org" ><dfn>5: </dfn>Forum</a></li>
                    <li class="active01"><dfn>6: </dfn><h3>Development</h3>
                    </li>
                  </ul>

                </div>

              </div>

            </div>

            <!-- End Horizontal Menu -->

            <hr />
          </div>
        </div>

        <!-- End Header -->


        <!-- Start Search-->
        <div id="search">
          <div style="float: right; margin: 0 0 0 1.5em;">

            <form action="/search/" method="get"><select name="type_of_search"><option value="soft">Software/Group</option>
              <option value="people">People</option>
              <option value="skill">Skill</option>
            </select><input type="text" size="12" name="words" value="" /><input type="submit" name="Search" value="Search" /></form>   
          </div>	
          <div style="margin-top: 0.5em;">
            <% session[:return_to] = request.request_uri unless @current_controller == 'account' -%>
            <% if !logged_in? -%>
            <%= link_to 'Login', :controller => 'account', :action => 'login' -%>
            <% else -%>
            Logged in as: <%= current_user.login -%> -- <a href="/account/logout">Logout</a>
            <% end -%>
          </div>

        </div>


        <!-- End Search -->

        <!-- Start Breadcrumbs -->
        <div class="breadcrumbs">
          You are here: <strong>Development Forge</strong>
          <hr />
        </div>
        <!-- End Breadcrumbs -->

        <!-- Start Content (Navigation and Content columns) -->
        <div id="content" class="clearfix">
          <h2 style="margin-bottom: 0; background-color: #30529C;">CMSMS Forge</h2>
          <div id="mainnav" class="clearfix">

            <!-- start tabs -->

            <ul>
              <li ><a class="tablink" href="/">Home</a></li>
              <li ><a class="tablink" href="/my/">My&nbsp;Page</a></li>
              <!-- <li ><a class="tablink" href="/softwaremap/">Project&nbsp;Tree</a></li> -->
              <% unless @project.nil? -%>
                <li class="active"><a class="tabsellink" href="<%= @project.home_page -%>"><%= @project.name -%></a></li>
              <% end -%>
            </ul>


            <!-- end tabs -->
          </div>

          <div id="main_content" class="clearfix" style="padding-top: 1.5em;">

            <!-- Inner Tabs / Shell -->
            <% unless @project.nil? -%>
              <div align="center"><h3 style="margin-top: 0;"><%= @project.name -%></h3></div>

              <div id="projectnav">
               
                <%= render :partial => '/project/project_nav', :project => @project -%>

              </div>
            <% end -%>

            <div id="flash">
              <% for name in [:notice, :warning, :message] %>
                <% if flash[name] %>
                  <%= "<div id=\"#{name}\">#{flash[name]}</div>" %>
                <% end %>
              <% end %>
            </div>

            <!-- <div id="content"> -->
              <%= yield -%>
            <!-- </div> -->

          </div>
        </div>
  </div>
  <div id="adsense" class="clearfix">


    <!-- Begin Google AdSense Ad -->

    <script type="text/javascript"><!--
    google_ad_client = "pub-7958561174011032";
    google_ad_width = "728";
    google_ad_height = "90";
    google_ad_format = "728x90_as";
    google_ad_channel = "";
    //--></script>
    <script type="text/javascript" src="http://pagead2.googlesyndication.com/pagead/show_ads.js"></script>

    <!-- End Google AdSense Ad -->


  </div>

  <!-- Start Footer -->

  <div id="footer" class="clearfix">
    <p>CMS Made Simple and the CMS Made Simple Forge are Free software under the GNU/GPL licence. Copyright &copy; <!--%= Now.strftime('%Y') -%--> 2007 Ted Kulp.<br />
	Other modules and plug-ins are Copyright by their authors, and licensed as specified.
    </p>
  </div>
  <!-- End Footer -->
  <div id="spinner" style="display: none;">
    <img alt="Spinner" src="/images/spinner.gif?1174174309" />
  </div>
</body>
</html>
